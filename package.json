    "connect-history-api-fallback": "^1.3.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-fetch-mock": "^1.2.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "open": "^0.0.5",
    "opn-cli": "^3.1.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.28",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-md5-hash": "^0.0.5",
    "yarn-run-all": "^3.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(css|scss|jpg|png|gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/mocks/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/tools/mocks/LocalStorageMock.js",
      "<rootDir>/tools/mocks/documentMock.js",
      "<rootDir>/tools/mocks/setupJest.js"
    ]
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && yarn",
    "start-message": "babel-node tools/startMessage.js",
    "prestartdev": "yarn run start-message",
    "startdev": "concurrently -k -r -s first \"yarn run test:watch\" \"yarn run open:client\" \"yarn run lint:watch\"",
    "open:client": "babel-node tools/srcServer.js",
    "open:server": "nodemon -w ./server --exec \"babel-node ./server --presets env,stage-1\"",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "yarn run lint --watch",
    "prebuild": "yarn run clean-dist && yarn run lint && yarn run test",
    "build": "babel-node tools/build.js",
    "clean-dist": "yarn run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prestart": "yarn run build",
    "start": "babel-node ./server --presets env,stage-1",
    "test": "jest",
    "test:cover": "yarn run test --coverage",
    "test:watch": "yarn run test --watch",
    "open:cover": "yarn run test:cover && babel-node tools/openCover.js",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  }
}